.components-back-app{left:10rpx;position:fixed;display:flex;flex-direction:row;z-index:22;align-items:center;height:60rpx;margin-top:10rpx}.components-back-app .button{height:60rpx}.components-back-app .button .custom-button{height:60rpx;padding:0 50rpx 0 16rpx;border-radius:10rpx;font-size:24rpx}.components-back-app .button .custom-button::before{content:'';width:12rpx;height:12rpx;margin-right:calc(12rpx / 2);transform:rotate(45deg);border-top:currentColor 1px solid;border-right:currentColor 1px solid;transform:rotate(-135deg)}.components-back-app .close{right:0;top:0;bottom:0;position:absolute;display:flex;flex-direction:row;align-items:center;justify-content:center;width:56rpx;color:#fff;font-size:24rpx;line-height:60rpx}
